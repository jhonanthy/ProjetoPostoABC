  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfrmFormPrincipal, frmFormPrincipal);
  Application.CreateForm(TfrmRelatorioAbastecimento, frmRelatorioAbastecimento);
