program ProjetoPostoABC;

uses
  Vcl.Forms,
  UnFormPrincipal in 'UnFormPrincipal.pas' {frmFormPrincipal},
  UnDataModule in 'UnDataModule.pas' {frmDataModule},
  UnEnum in 'UnEnum.pas';

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  AApplication.CreateForm(TfrmFormPrincipal, frmFormPrincipal);
  AApplication.CreateForm(TfrmDataModule, frmDataModule);
  plication.Run;
end.
